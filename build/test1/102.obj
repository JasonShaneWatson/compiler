STRT  NOP          stage1no102 - Aspen Wills, Jason Watson
      RDI I2        read(a)
      RDI I3        read(b)
      LDA ZERO  
      ISB I1    
      IDV I2    
      STQ T1    
      LDA T1    
      IAD I3        T1 + b
      STA I6          z := T1
      PRI I6        write(z)
      IAD I3        z + b
      STA T1    
      LDA I2    
      ISB I3          a - b
      IMU T1        T2 * T1
      STA I4          c := T1
      PRI I4        write(c)
      HLT       
I0    DEC 0000      zero           
I1    DEC 0005      five           
I2    BSS 0001      a              
I3    BSS 0001      b              
I4    BSS 0001      c              
I5    BSS 0001      w              
I6    BSS 0001      z              
ZERO  DEC 0000      ZERO           
T1    BSS 0001      T1             
      END STRT  
