STRT  NOP          stage1no105 - Aspen Wills, Jason Watson
      RDI I2        read(a)
      RDI I3        read(b)
      RDI I4        read(c)
      RDI I6        read(z)
      LDA I3    
      IDV I2    
      STQ T0    
      LDA T0    
      STA I6          z := T0
      IMU I4        z * c
      STA I5          w := T0
      LDA ZERO  
      ISB I6    
      STA I2          a := T0
      LDA I4    
      STA I3          b := c
      PRI I6        write(z)
      PRI I5        write(w)
      PRI I2        write(a)
      PRI I3        write(b)
      HLT       
I0    DEC 0000      zero           
I1    DEC 0005      five           
I2    BSS 0001      a              
I3    BSS 0001      b              
I4    BSS 0001      c              
I5    BSS 0001      w              
I6    BSS 0001      z              
ZERO  DEC 0000      ZERO           
      END STRT  
