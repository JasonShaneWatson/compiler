STRT  NOP          stage1no147 - Aspen Wills, Jason Watson
      LDA I4    
      IAD I5        3 + 34
      IMU I0        T0 * five
      STA I1          a := T0
      LDA ZERO  
      ISB I0    
      IDV I1    
      STQ T1    
      LDA T1    
      IAD I1        T1 + a
      STA I2          b := T1
      IAD I1        b + a
      STA T1    
      LDA I2    
