STRT  NOP          stage1no135 - Aspen Wills, Jason Watson
      LDA TRUE  
      STA B0          a := TRUE
      AZJ L0         not B0
      LDA FALS  
      UNJ L0  +1   
L0    LDA TRUE  
      STA B1          b := T0
      PRI B0        write(a)
      PRI B1        write(b)
      LDA FALS  
      STA B1          b := FALSE
      PRI B1        write(b)
      LDA TRUE  
      STA B1          b := TRUE
      PRI B1        write(b)
      ISB B0         B0 = B1
      AZJ L1         
      LDA FALS  
      UNJ L1  +1   
L1    LDA TRUE  
      AZJ L2         not T0
      LDA FALS  
      UNJ L2  +1   
L2    LDA TRUE  
      STA B1          b := T0
      PRI B0        write(a)
      PRI B1        write(b)
      HLT       
B0    BSS 0001      a              
B1    BSS 0001      b              
TRUE  DEC 0001      TRUE           
FALS  DEC 0000      FALSE          
      END STRT  
